{
	"extends": "astro/tsconfigs/strict",
	"include": [".astro/types.d.ts", "**/*"],
	"exclude": ["dist"],
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"baseUrl": "./src",
		"paths": {
			"@/*": ["src/*"],
			"@components/*": ["./components/*"],
			"@layouts/*": ["./layouts/*"],
			"@pages/*": ["./pages/*"],
			"@icons/*": ["./assets/icons/*"],
			"@lib/*": ["./lib/*"],
			"@typs/*": ["./types/*"]
		}
	}
}
